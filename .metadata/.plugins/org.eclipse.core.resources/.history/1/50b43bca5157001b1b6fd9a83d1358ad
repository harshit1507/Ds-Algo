import java.io.*;
public class CreateFile {

	public static void main(String[] args) throws IOException {
		// TODO Auto-generated method stub
		File f = new File("D:\\PrepBytes\\File Handling\\Java\\testFile.txt");
		System.out.println(f.exists());
		
		f.createNewFile();
		
		System.out.println(f.exists());
		
	}

}
